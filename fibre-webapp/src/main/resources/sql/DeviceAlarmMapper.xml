  <select id="getUnhandledAlarmCountByDeviceCode" parameterType="java.lang.String" resultType="java.lang.Integer">
  select
  count(*)
  from xt_device_alarm
  where device_code = #{deviceCode,jdbcType=VARCHAR}
  and `status` = 0
  and `df` = 0
  </select>



  <update id="updateAlarmStatus">
    update xt_device_alarm
    <set>
      <if test="status != null">
        status = #{status},
      </if>
      update_time = now(),
    </set>
    where device_code = #{deviceCode,jdbcType=VARCHAR}
    and df = 0
  </update>

  <update id="updateRealFall">
    update xt_device_alarm
    <set>

      real_fall = 1 - real_fall,

      update_time = now(),
    </set>
    where id = #{id}
    and df = 0
  </update>

  <update id="updateAlarmType">
    update xt_device_alarm
    <set>
      type = #{type},
      update_time = now(),
    </set>
    where id = #{id}
    and df = 0
  </update>


  <update id="updateDescription">
    update xt_device_alarm
    <set>
      description = #{description},
      update_time = now(),
    </set>
    where id = #{id}
    and df = 0
  </update>





  <select id="getAll" parameterType="java.lang.String" resultMap="BaseResultMap">
    select *
    from xt_device_alarm
    where `df` = 0 and vendor_id = #{vendorId}
    order by create_time DESC
  </select>


  <select id="getDataCountByCondition" resultType="java.lang.Integer">
    select count(*)
    from xt_device_alarm
    where df = 0
    <if test="vendorId!=null">
      and vendor_id = #{vendorId}
    </if>
    <if test="deviceCode!=null and deviceCode!=''">
      and device_code = #{deviceCode}
    </if>
    <if test="status!=null">
      and status = #{status}
    </if>
  </select>

  <select id="getDataByCondition" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from xt_device_alarm
    where df = 0
    <if test="vendorId!=null">
      and vendor_id = #{vendorId}
    </if>
    <if test="deviceCode!=null and deviceCode!=''">
      and device_code = #{deviceCode}
    </if>
    <if test="status!=null">
      and status = #{status}
    </if>
    order by create_time DESC
    <if test="start!=null">
      limit #{start}, #{end}
    </if>
  </select>





