  <insert id="insert" parameterType="com.optical.bean.DeviceAlarm">
    insert into xt_device_alarm ( vendor_id, device_code, type,
      confidence, reason, frame_id, 
      event_start_frame, event_end_frame, height_curr, 
      height_before, status, deal_user_id, 
      deal_detail, create_time, update_time, 
      deal_time, device_phone, phone_time, 
      device_name, device_type, device_version, 
      `count`, real_fall, df)
    values (#{vendorId, jdbcType=BIGINT}, #{deviceCode,jdbcType=VARCHAR}, #{type,jdbcType=INTEGER},
      #{confidence,jdbcType=INTEGER}, #{reason,jdbcType=INTEGER}, #{frameId,jdbcType=BIGINT},
      #{eventStartFrame,jdbcType=BIGINT}, #{eventEndFrame,jdbcType=BIGINT}, #{heightCurr,jdbcType=DOUBLE},
      #{heightBefore,jdbcType=DOUBLE}, #{status,jdbcType=INTEGER}, #{dealUserId,jdbcType=BIGINT},
      #{dealDetail,jdbcType=VARCHAR}, now(), null,
      #{dealTime,jdbcType=TIMESTAMP}, #{devicePhone,jdbcType=VARCHAR}, #{phoneTime,jdbcType=TIMESTAMP}, 
      #{deviceName,jdbcType=VARCHAR}, #{deviceType,jdbcType=BIGINT}, #{deviceVersion,jdbcType=INTEGER}, 
      #{count,jdbcType=INTEGER}, 0,  #{df,jdbcType=INTEGER})
  </insert>


  <select id="getUnhandledAlarmCount"  resultType="java.lang.Integer">
    select count(*)
    from xt_device_alarm
    where `status` = 0
    <if test="vendorId!=null">
      and vendor_id = #{vendorId}
    </if>
    and `df` = 0
  </select>


  <select id="getAlarmCountSinceToday"  resultType="java.lang.Integer">
    select count(*)
    from xt_device_alarm
    where `df` = 0
    <if test="vendorId!=null">
      and vendor_id = #{vendorId}
    </if>
    and   <![CDATA[ create_time > #{startTime} ]]>
  </select>


